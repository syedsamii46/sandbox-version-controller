    <validationRules>
        <fullName>contact_details_is_must</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
   ISBLANK (contact_detail__c)
   
)</errorConditionFormula>
        <errorMessage>cant save without contact details</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>patient_email_cant_be_updated</fullName>
        <active>true</active>
        <errorConditionFormula>ISCHANGED(  Patient_Email__c )</errorConditionFormula>
        <errorMessage>email cant be updated</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_within_current_month_and_year</fullName>
        <active>true</active>
        <errorConditionFormula>OR (
YEAR(  Registration_Date__c  ) &lt;&gt; YEAR ( TODAY() ),
MONTH(  Registration_Date__c  ) &lt;&gt; MONTH ( TODAY() )
)</errorConditionFormula>
        <errorMessage>Date must be in the current month.</errorMessage>
    </validationRules>
</CustomObject>